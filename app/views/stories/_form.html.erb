<%= render "shared/error_messages", record: @story %>
<div class="form-group">
  <%= f.label :url, "URL" %>
  <%= f.text_field :url, class: "form-control", placeholder: "URL do Bizu!", disabled: disabled_field(story), autofocus: true %>
</div>
<div class="form-group">
  <%= f.label :description, "Descrição" %>
  <%= f.text_area :description, rows: 7, cols: 15, class: "form-control", placeholder: "Deixe seu Bizuuuuuuu!" %>
</div>
